 #include<bits/stdc++.h>
 using namespace std;
 int main()
 {
     int n;
     cin>>n;
     if(n<=1917)
     {
         if(n%4==0) printf("12.09.%d",n);
         else printf("13.09.%d",n);
     }
     else if(n==1918) printf("26.09.%d",n);
     else
     {
         if(n%400==0 || (n%4==0 && n%100!=0))
         {
             if(n%4==0) printf("12.09.%d",n);
         }
         else printf("13.09.%d",n);
     }
     

return 0;

 }
