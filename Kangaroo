#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x1,x2,v1,v2,i;
    bool fg = false;
    cin>>x1>>v1;
    cin>>x2>>v2;
    while(x1<x2)
    {
        x1 += v1;
        x2 += v2;
        if(x1==x2)
        {
            fg = true;
            break;
        }
        else if(x1>x2)
        {
            break;
        }
    }
    if(fg) cout<<"YES";
    else cout<<"NO";

}
