#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define i64 long long int
const double pi = 2*acos(0) ;
#define mx 10000001

int main()
{

    int n,k,q,i,m;
    cin>>n>>k>>q;
    int arr[n];
    int j = k%n;
    for(i = 0 ; i<n ; i++)
    {
        if(j == n) j = 0;
        cin>>arr[j];
        j++;
    }

    while(q--)
    {
        cin>>m;
        cout<<arr[m]<<"\n";
    }

return 0;
}


