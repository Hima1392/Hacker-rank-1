#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a[3],b[3],i;
    int acnt=0,bcnt=0;

    for(i=0;i<3;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<3;i++)
    {
       cin>>b[i];
    }
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i]) acnt++;
        else if(b[i]>a[i]) bcnt++;
    }
    cout<<acnt<<" "<<bcnt<<endl;
  return 0;
}
