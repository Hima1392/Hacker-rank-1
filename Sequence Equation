#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define i64 long long int
const double pi = 2*acos(0) ;
#define mx 10000001
bool status[32000] = {0};

int main()
{
    int n,i,j;
    cin>>n;
    int arr[n+1];
    for(i = 1 ; i<=n ;i++)
    {
        cin>>arr[i];
    }
    for(i = 1 ; i<=n ; i++)
    {
        for(j = 1 ; j<=n ; j++)
        {
            if(arr[arr[j]] == i)
            {
                cout<<j<<"\n";
                break;
            }
        }
    }

return 0;
}
