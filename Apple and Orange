#include<bits/stdc++.h>
using namespace std;
int main()
{
    int s,t,a,b,m,n,i,x,cntm=0,cntn=0;
    cin>>s>>t;
    cin>>a>>b;
    cin>>m>>n;
    for(i=0;i<m;i++)
    {
        cin>>x;
        if((x+a) >= s && (x+a) <= t) cntm++;
    }
    for(i=0;i<n;i++)
    {
        cin>>x;
        if((x+b) >= s && (x+b) <= t) cntn++;
    }
    cout<<cntm<<endl<<cntn;

return 0;
}
