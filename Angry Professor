#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define i64 long long int
const double pi = 2*acos(0);


int main()
{

    int t,n,k;
    cin>>t;
    while(t--)
    {
        cin>>n>>k;
        int a,stdnt = 0;
        while(n--)
        {
            cin>>a;
            if(a<=0) stdnt++;
        }
        if(stdnt<k) pf("YES\n");
        else pf("NO\n");

    }
return 0;
}
